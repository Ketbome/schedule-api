{
  "name": "ripley-agenda-api",
  "version": "1.0.0",
  "description": "API de agenda de despacho",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
